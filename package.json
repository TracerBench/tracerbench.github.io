  "author": "Kris Selden <kris.selden@gmail.com>",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "yarn lint:js && ember build",
    "lint:js": "./node_modules/.bin/eslint ./app/**/*.js",
    "lint:hbs": "./node_modules/.bin/ember-template-lint ./app/**/*.hbs",
    "start": "ember serve",
    "test": "ember test",
    "deploy": "ember build --environment production && ember github-pages:commit --message \"chore: deploy to master from commit $(git rev-parse HEAD)\" --branch=\"master\"",
    "tb:compare": "ember build --environment=production && ./scripts/tb.js"
  },
  "devDependencies": {
    "@ember/optional-features": "^1.3.0",
    "babel-eslint": "^8.0.0",
    "broccoli-asset-rev": "^3.0.0",
    "ember-ajax": "^5.0.0",
    "ember-cli": "~3.17.0",
    "ember-cli-app-version": "^3.2.0",
    "ember-cli-babel": "^7.19.0",
    "ember-cli-dependency-checker": "^3.2.0",
    "ember-cli-eslint": "^5.1.0",
    "ember-cli-github-pages": "^0.2.2",
    "ember-cli-htmlbars": "^4.2.3",
    "ember-cli-inject-live-reload": "^2.0.2",
    "ember-cli-markdown-resolver": "^0.1.3",
    "ember-cli-sass": "^10.0.1",
    "ember-cli-showdown": "^4.5.0",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-uglify": "^3.0.0",
    "ember-code-snippet": "^3.0.0",
    "ember-decorators": "^6.1.1",
    "ember-export-application-global": "^2.0.1",
    "ember-fetch": "^8.0.1",
    "ember-load-initializers": "^2.1.1",
    "ember-maybe-import-regenerator": "^0.1.6",
    "ember-prism": "^0.6.1",
    "ember-qunit": "^4.6.0",
    "ember-resolver": "^7.0.0",
    "ember-source": "~3.17.2",
    "ember-template-lint": "^2.5.2",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-ember": "^8.1.0",
    "eslint-plugin-hbs": "^1.0.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.1",
    "fs-extra": "^9.0.0",
    "loader.js": "^4.7.0",
    "path": "^0.12.7",
