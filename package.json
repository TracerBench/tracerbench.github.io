{
  "name": "tracerbench",
  "version": "0.0.0",
  "private": true,
  "description": "TracerBench.com",
  "license": "BSD-2-Clause",
  "author": "Kris Selden <kris.selden@gmail.com>",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "yarn lint && ember build",
    "lint": "yarn lint:js && yarn link:hbs",
    "lint:js": "./node_modules/.bin/eslint ./app/**/*.js",
    "lint:hbs": "./node_modules/.bin/ember-template-lint ./app/**/*.hbs",
    "start": "ember serve",
    "test": "ember test",
    "deploy": "ember build --environment production && ember github-pages:commit --message \"chore: deploy to master from commit $(git rev-parse HEAD)\" --branch=\"master\"",
    "fetch-markdown": "rm -rf markdown && wget https://github.com/TracerBench/tracerbench/archive/master.zip && unzip -qq -j master.zip 'tracerbench-master/packages/cli/markdown/*' -d markdown && rm -rf master.zip && git add markdown"
  },
  "devDependencies": {
    "@ember/optional-features": "^1.3.0",
    "babel-eslint": "^10.1.0",
    "broccoli-asset-rev": "^3.0.0",
    "ember-ajax": "^5.0.0",
    "ember-cli": "~3.20.0",
    "ember-cli-app-version": "^3.2.0",
    "ember-cli-babel": "^7.22.1",
    "ember-cli-dependency-checker": "^3.2.0",
    "ember-cli-eslint": "^5.1.0",
    "ember-cli-github-pages": "^0.2.2",
    "ember-cli-htmlbars": "^5.3.1",
    "ember-cli-inject-live-reload": "^2.0.2",
    "ember-cli-markdown-resolver": "^0.1.3",
    "ember-cli-sass": "^10.0.1",
    "ember-cli-showdown": "^4.5.0",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-uglify": "^3.0.0",
    "ember-code-snippet": "^3.0.0",
    "ember-decorators": "^6.1.1",
    "ember-export-application-global": "^2.0.1",
    "ember-fetch": "^8.0.2",
    "ember-load-initializers": "^2.1.1",
    "ember-maybe-import-regenerator": "^0.1.6",
    "ember-prism": "^0.8.0",
    "ember-qunit": "^4.6.0",
    "ember-resolver": "^8.0.2",
    "ember-source": "~3.21.0",
    "ember-template-lint": "^2.10.0",
    "eslint": "^7.7.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-ember": "^8.12.0",
    "eslint-plugin-hbs": "^1.0.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "fs-extra": "^9.0.1",
    "har-remix": "^2.3.0",
    "husky": "^4.3.0",
    "loader.js": "^4.7.0",
    "path": "^0.12.7",
    "prettier": "^2.1.1",
    "qunit-dom": "^1.4.0",
    "sass": "^1.32.12",
    "tracerbench": "^4.1.1"
  },
  "engines": {
    "node": ">= 10"
  },
  "husky": {
    "hooks": {
      "pre-push": "yarn fetch-markdown"
    }
  }
}
